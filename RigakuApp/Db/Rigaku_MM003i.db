record(ai, "$(P)$(R)voltage_RBV")
{
    field(DTYP, "asynFloat64")
    field(INP,  "@asyn($(PORT),$(ADDR))VOLTAGE_IN_VALUE")
    field(LINR, "NO CONVERSION")
	field(PREC, "1")
	field(EGU,  "V")
    field(SCAN, "I/O Intr")
}

